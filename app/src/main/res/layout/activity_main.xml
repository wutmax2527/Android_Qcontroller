<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <Button
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:id="@+id/btn_ClearDebug"
                android:layout_margin="10dp"
                android:text="Clear Debug"/>
            <Button
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:id="@+id/btn_Setting"
                android:layout_margin="10dp"
                android:text="Setting"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/btn_GetQ"
                android:layout_margin="5dp"
                android:textSize="10sp"
                android:text="GetQ"/>
            <EditText
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:layout_margin="5dp"
                android:id="@+id/edtText_Div"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/btn_ResetQ"
                android:layout_margin="5dp"
                android:textSize="10sp"
                android:text="Reset Q"/>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/btn_ShowTouch"
                android:layout_margin="5dp"
                android:textSize="10sp"
                android:text="Show"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:orientation="horizontal">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/btn_login"
                android:layout_margin="5dp"
                android:textSize="10sp"
                android:text="Login"/>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/btn_logout"
                android:layout_margin="5dp"
                android:textSize="10sp"
                android:text="Logout"/>
            <EditText
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:layout_margin="5dp"
                android:textSize="12sp"
                android:id="@+id/editText_Code"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/btn_Cancel"
                android:layout_margin="5dp"
                android:textSize="10sp"
                android:text="Cancel"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:orientation="horizontal">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/btn_NextQ"
                android:layout_margin="2dp"
                android:textSize="10sp"
                android:text="Next"/>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/btn_RecallQ"
                android:layout_margin="2dp"
                android:textSize="10sp"
                android:text="Recall"/>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/btn_Hold"
                android:layout_margin="2dp"
                android:textSize="10sp"
                android:text="Hold"/>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/btn_CallHD"
                android:layout_margin="2dp"
                android:textSize="10sp"
                android:text="CallHD"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:orientation="horizontal">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/btn_Finish"
                android:layout_margin="5dp"
                android:textSize="10sp"
                android:text="FinishQ"/>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/btn_GroupQ"
                android:layout_margin="5dp"
                android:textSize="10sp"
                android:text="Group"/>
            <EditText
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:layout_margin="5dp"
                android:textSize="12sp"
                android:id="@+id/editText_StartQ"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:textSize="30sp"
                android:layout_margin="5dp"
                android:text="-"/>
            <EditText
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:layout_margin="5dp"
                android:textSize="12sp"
                android:id="@+id/editText_EndQ"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/btn_Test1"
                android:layout_margin="5dp"
                android:textSize="10sp"
                android:text="Test1"/>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/btn_Test2"
                android:layout_margin="5dp"
                android:textSize="10sp"
                android:text="Test2"/>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/btn_SendData"
                android:layout_margin="5dp"
                android:textSize="10sp"
                android:text="Send"/>
            <EditText
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:layout_margin="5dp"
                android:textSize="12sp"
                android:id="@+id/editText_Serial"
                android:text="Test..."
                />


        </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/tv_MessageDebug"
            android:text="Message"
            android:scrollbars="vertical"
            />

    </LinearLayout>

</LinearLayout>